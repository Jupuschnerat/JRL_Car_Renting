  <%= simple_form_for [@car] do |f| %>
  <div class="row justify-content-center">
    <div class="col-6">
      <%= f.input :model %>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-6">
      <%= f.input :description %>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-6">
      <%= f.input :price %>
    </div>
  </div>
   <div class="row justify-content-center">
    <div class="col-6">
      <%= f.input :city %>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-8">
      <%= f.input :photo, as: :file %>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-8">
      <%= f.input :category, collection: %w[ sport suv suvw classic minivan ] %>
    </div>
  </div>
    <%= f.submit "Submit car", class: "btn btn-primary border my-5" %>
  <% end %>
